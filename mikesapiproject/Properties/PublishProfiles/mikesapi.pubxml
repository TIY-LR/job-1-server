<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://mikesapi.azurewebsites.net</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>mikesapi.scm.azurewebsites.net:443</MSDeployServiceURL>
    <DeployIisAppPath>mikesapi</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>$mikesapi</UserName>
    <_SavePWD>True</_SavePWD>
    <_DestinationType>AzureWebSite</_DestinationType>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="DefaultConnection" Order="1" Enabled="True">
          <Destination Path="Data Source=tcp:mikedatabase.database.windows.net,1433;Initial Catalog=mikesapi_db;User ID=tiyclass@mikedatabase;Password=MEi$tw4c" Name="" />
          <Object Type="DbCodeFirst">
            <Source Path="DBMigration" DbContext="mikesapiproject.Models.ApplicationDbContext, mikesapiproject" MigrationConfiguration="mikesapiproject.Migrations.Configuration, mikesapiproject" Origin="Configuration" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="PostsDbContext" Order="2" Enabled="False">
          <Destination Path="Data Source=tcp:mikedatabase.database.windows.net,1433;Initial Catalog=mikesapi_db;User ID=tiyclass@mikedatabase;Password=MEi$tw4c" Name="" />
          <Object Type="DbDacFx">
            <PreSource Path="Data Source=(LocalDB)\MSSQLLocalDB;AttachDbFilename=|DataDirectory|\posts.mdf;Integrated Security=True;Connect Timeout=30;Application Name=EntityFramework" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\PostsDbContext_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="data source=(LocalDB)\MSSQLLocalDB;attachdbfilename=|DataDirectory|\posts.mdf;integrated security=True;connect timeout=30;MultipleActiveResultSets=True;App=EntityFramework" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
          </UpdateFrom>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
    <ADUsesOwinOrOpenIdConnect>False</ADUsesOwinOrOpenIdConnect>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)DefaultConnection-Web.config Connection String">
      <ParameterValue>Data Source=tcp:mikedatabase.database.windows.net,1433;Initial Catalog=mikesapi_db;User ID=tiyclass@mikedatabase;Password=MEi$tw4c</ParameterValue>
    </MSDeployParameterValue>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)PostsDbContext-Web.config Connection String">
      <ParameterValue>data source=tcp:mikedatabase.database.windows.net,1433;initial catalog=mikesapi_db;user id=tiyclass@mikedatabase;password=MEi$tw4c;MultipleActiveResultSets=True;App=EntityFramework</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>